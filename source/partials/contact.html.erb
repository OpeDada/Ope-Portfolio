        <div class="navigation-button d-flex flex-column justify-content-center align-items-center">
          <i class="fab fa-github-square" style="font-size: 28px;"></i>
          <%= link_to "", "https://github.com/OpeDada", target: '_blank', class: 'navigation-button-link' %>
