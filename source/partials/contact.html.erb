        <div class="navigation-button d-flex flex-column justify-content-center align-items-center">
          <i class="fab fa-linkedin" style="font-size: 28px;"></i>
          <%= link_to "", "https://www.linkedin.com/in/opemipo-dada/", target: '_blank', class: 'navigation-button-link' %>
